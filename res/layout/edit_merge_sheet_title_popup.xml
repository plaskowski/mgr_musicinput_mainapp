<?xml version="1.0" encoding="utf-8"?>
<merge 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:custom="http://schemas.android.com/apk/res/pl.edu.mimuw.students.pl249278.android.musicinput"
>
	<pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.RelativeLayout_InterceptScale_LockTouchInput
		android:id="@+id/EDIT_scale_detector"
		style="@style/EDIT_scale_detector"
	>
	    <pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.HorizontalScrollView_InterceptOnScrollChanged
	    	android:id="@id/ShowScore_horizontal_scrollview"
	    	android:layout_width="fill_parent" 
	    	android:layout_height="fill_parent"
	    >
	    	<pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.HackedScrollView
	    		android:id="@+id/EDIT_vertscrollview"
		    	android:layout_width="wrap_content" 
	    		android:layout_height="fill_parent"
	    	>
	    		<pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.HackedScrollViewChild
			    	android:layout_width="wrap_content" 
		    		android:layout_height="wrap_content"
		    		android:orientation="vertical"
    		    >
					<pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.AbsoluteLayout_CorrectTopPosition_StaveHighlighter
						android:id='@+id/EDIT_sheet_container'  
				    	android:background="@android:color/white"
				    	android:layout_width="fill_parent"
				    	android:layout_height="wrap_content"
				    	android:visibility="invisible"
				   	>
				   		<pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.Sheet5LinesView
				   		android:id='@+id/EDIT_sheet_5lines'  
				    	android:layout_width="fill_parent"
				    	android:layout_height="fill_parent"
				   		/>
				   	</pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.AbsoluteLayout_CorrectTopPosition_StaveHighlighter>
			   	</pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.HackedScrollViewChild>
			</pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.HackedScrollView>
	    </pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.HorizontalScrollView_InterceptOnScrollChanged>
	    
	    <pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.View_ExtendedBackground_CacheDraw
		    android:id="@+id/EDIT_inputArea"
		    android:layout_width="@dimen/inputAreaWidth"
		    android:layout_gravity="right"
		    android:layout_height="fill_parent"
		    android:layout_marginRight="@dimen/inputAreaRightMargin"
		    android:layout_alignParentRight="true"
		    custom:extendedBackground="@style/iaFrame"
	    />
	    
	   	<pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.ImageButton_ExtendedImage
		    android:id="@+id/EDIT_button_insert"
		    android:layout_width="48dip"
		    android:layout_height="48dip"
		    android:background="@drawable/button_bluegradient"
		    custom:extendedImage="@style/insertButton"
		    android:scaleType="fitXY"
		    custom:iconMaxHeight="28dip"
		    style="@style/EDIT_button_insert"
	    />
   
		<include 
	    	layout="@layout/editscreen_title"
	    	android:id="@+id/EDIT_title"
	    	style="@style/EDIT_title"
	   	/>
	   		    	    
	    <pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.QuickActionsView 
	    	android:orientation="horizontal"
	    	android:id="@+id/EDIT_quickactions"
	    	android:layout_width="wrap_content"
	    	android:layout_height="wrap_content"
	    	android:layout_marginLeft="30dip"
	    	android:layout_marginTop="50dip"
		    android:layout_gravity="left|top"
		    custom:buttonStyle="@layout/button_template"
		    custom:toggleButtonStyle="@layout/statebutton_template"
		    custom:extendedBackground="@style/TooltipBg1"
    		custom:buttonIconPaintSetup="@array/InvertedEmboss"
    		custom:inactiveButtonIconPaintSetup="@style/inactive_button_icon_dark"
	    />
   </pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.RelativeLayout_InterceptScale_LockTouchInput>
	
   <FrameLayout
		android:id="@+id/EDIT_info_popup"
		style="@style/EDIT_info_popup"
		android:layout_alignWithParentIfMissing="true"
		android:layout_alignParentTop="true"
		android:layout_alignParentRight="true"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:visibility="gone"
   >
	   <pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.LinearLayout_ExtendedBackground
	   		android:orientation="vertical"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_gravity="center" 
			custom:extendedBackground="@style/infoPopupBg"
		><!-- popup view displaying note value -->
			<TextView
				android:text="@string/EDIT_current_note_value"
				android:textSize="20sp"
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content"
				android:layout_gravity="center_horizontal"
				android:textColor="#666666"
				android:layout_marginLeft="4dip"
				android:layout_marginRight="4dip"
				android:layout_marginBottom="5dip"
			/>
			<pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.SheetAlignedElementView
				android:id="@+id/EDIT_info_popup_note"
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content"
				android:layout_gravity="center_horizontal"
				custom:paint="@style/infoPopupIconPaint"
				android:layout_marginBottom="4dip"
			/>
		</pl.edu.mimuw.students.pl249278.android.musicinput.ui.view.LinearLayout_ExtendedBackground>
    </FrameLayout>
</merge>