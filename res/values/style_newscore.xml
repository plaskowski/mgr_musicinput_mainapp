<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="NEWSCORE_h1" parent="prefs_label">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@drawable/topdivider_bright</item>
	</style>
    
    <style name="NEWSCORE_wrapperscroll">
        <item name="android:layout_marginBottom">10sp</item>
    </style>
    
    <dimen name="NEWSCORE_thumbs_lineThickness">2dip</dimen>
    
	<style name="NEWSCORE_thumbsPaint" parent="@style/PaintAntialias">
	    <item name="paintStyle">FILL</item>
	    <item name="color">#FFFFFF</item>
	</style>
    
    <style name="NEWSCORE_timesignature_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">50dip</item>
        <item name="android:minHeight">50dip</item>
        </style>
		
   <style name="NEWSCORE_frame" parent="RectDrawable">
       <item name="strokePaint">@style/Stroke2dp</item>
       <item name="strokeColorStateList">@color/selector_frame_stroke</item>
       <item name="fillPaint">@style/FillAntialias</item>
       <item name="fillColorStateList">@color/selector_frame_bg</item>
       <item name="padding">7dp</item>
       <item name="inset">1dp</item>
   </style>
   
   <style name="Stroke2dp" parent="@style/StrokeAntialias">
       <item name="strokeWidth">2dp</item>
   </style>
   
	<style name="StrokeAntialias" parent="@style/PaintAntialias">
	    <item name="paintStyle">STROKE</item>
	</style>
</resources>