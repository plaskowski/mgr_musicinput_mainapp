<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <style name="StyleResourceMockup" />
    
    <declare-styleable name="CustomizableView">
		<attr name="maxHeight" format="dimension" />
	</declare-styleable>
    
    <declare-styleable name="SheetElementView">
		<attr name="paint" format="reference" />
	</declare-styleable>
	
    <declare-styleable name="LinedSheetElementView">
		<attr name="minLinesWidth" format="dimension" />
		<attr name="linesHorizontalPadding" format="dimension" />
	</declare-styleable>
    
    <declare-styleable name="NoteValueSpinner">
		<attr name="itemPaint" format="reference" />
		<attr name="selectedItemPaint" format="reference" />
		<attr name="spacing" format="float" />
	</declare-styleable>
    	
	<declare-styleable name="ExtendedBackground">
		<attr name="extendedBackground" format="reference" />
	</declare-styleable>
	
	<declare-styleable name="ExtendedTextDrawable">
		<attr name="extendedDrawableLeft" format="reference" />
	</declare-styleable>

	<declare-styleable name="ExtendedImage">
		<attr name="extendedImage" format="reference" />
	</declare-styleable>
	
   	<declare-styleable name="CompoundDrawable">
	    <!-- array of references to PaintSetup styles -->
		<attr name="paintsSetup" format="reference" />
		<attr name="additionalPadding" format="dimension" />
	</declare-styleable>
	
	<declare-styleable name="RectDrawable">
	    <attr name="fillPaint" format="reference" />
	    <attr name="strokePaint" format="reference" />
	    <attr name="fillColorStateList" format="color" />
	    <attr name="strokeColorStateList" format="color" />
   	</declare-styleable>
   		    	
	<declare-styleable name="SvgDrawable">
		<attr name="svgResource" format="reference" />
		<attr name="iconMaxHeight" format="dimension" />
		<attr name="intrinsicWidth" format="dimension" />
		<attr name="intrinsicHeight" format="dimension" />
	</declare-styleable>
	
	<declare-styleable name="Corners">
	    <attr name="cornerTopLeft" format="dimension" />
	    <attr name="cornerTopRight" format="dimension" />
	    <attr name="cornerBottomLeft" format="dimension" />
	    <attr name="cornerBottomRight" format="dimension" />
   	</declare-styleable>
   	
	<declare-styleable name="Padding">
	    <attr name="padding" format="dimension" />
	    <attr name="paddingLeft" format="dimension" />
	    <attr name="paddingTop" format="dimension" />
   	    <attr name="paddingRight" format="dimension" />
   	    <attr name="paddingBottom" format="dimension" />	    	    	    
	</declare-styleable>
	
	<declare-styleable name="Inset">
	    <attr name="insetLeft" format="dimension" />
	    <attr name="insetTop" format="dimension" />
	    <attr name="insetRight" format="dimension" />
	    <attr name="insetBottom" format="dimension" />
	</declare-styleable>
	    	
	<declare-styleable name="StyleableClassDeclaration">
		<attr name="className" format="string" />
	</declare-styleable>
	
	<declare-styleable name="QuickActions">
		<attr name="buttonStyle" format="reference" />
		<attr name="toggleButtonStyle" format="reference" />
		<attr name="buttonIconPaintSetup" format="reference" />
		<attr name="inactiveButtonIconPaintSetup" format="reference" />
	</declare-styleable>
	
	<declare-styleable name="TooltipStyle">
		<attr name="outlineCornerRadius" format="dimension" />
		<attr name="borderThickness" format="dimension" />
		<attr name="borderColor" format="color" />
		<attr name="fill" format="reference" />
		<attr name="indicatorSize" format="dimension" />
	</declare-styleable>
</resources>